{
    "name": "exewen/image-php-sdk",
    "description": "A image-php-sdk component for Exewen.",
    "authors": [
        {
            "name": "exewen",
            "email": "exeweb@163.com"
        }
    ],
    "license": "MIT",
    "type": "library",
    "keywords": [
        "php",
        "exewen",
        "image-sdk"
    ],
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "exewen/facades": "^1",
        "khanamiryan/qrcode-detector-decoder": "1.0.5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 | ^6.5 | ^7.5 | ^8.4 | ^9.3"
    },
    "autoload": {
        "psr-4": {
            "Exewen\\Image\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ExewenTest\\Image\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "extra": {
        "exewen": {
            "config": "Exewen\\Image\\ConfigRegister"
        }
    }

}
